{
    "cleanUrls": true,
    "builds": [
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "public"
            }
        }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "/$1" }
    ],
    "functions": {
        "api/travel-rizz-server.js": {
            "maxDuration": 60
        },
        "api/travel-rizz-basic-info.js": {
            "maxDuration": 60
        },
        "api/travel-rizz-details.js": {
            "maxDuration": 60
        },
        "api/travel-rizz-daily-itinerary.js": {
            "maxDuration": 60
        },
        "api/travel-rizz-conclusion.js": {
            "maxDuration": 60
        },
        "api/maps-key.js": {
            "maxDuration": 10
        }
    }
}